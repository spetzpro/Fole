{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "copyrightText": { "type": "string" },
        "links": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "text": { "type": "string" },
                    "url": { "type": "string" }
                },
                "required": ["text", "url"]
            }
        }
    },
    "required": ["copyrightText"]
}
