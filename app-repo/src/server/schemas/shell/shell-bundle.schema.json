{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
        "manifest",
        "blocks"
    ],
    "properties": {
        "manifest": {
            "$ref": "shell-manifest.schema.json"
        },
        "blocks": {
            "type": "object",
            "patternProperties": {
                "^.*$": {
                    "$ref": "block-envelope.schema.json"
                }
            }
        }
    }
}